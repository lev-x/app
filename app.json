{
  "expo": {
    "name": "LevX",
    "slug": "levx",
    "scheme": "levx",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#291C64",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "app.levx",
      "googleServicesFile": "./google-services.json",
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "app.levx"
    },
    "notification": {
      "icon": "./assets/icon-dark.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    }
  }
}
